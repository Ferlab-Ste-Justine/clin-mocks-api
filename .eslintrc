{
    "extends": ["eslint:recommended", "prettier"],
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "prettier/prettier": "error",
      "arrow-body-style": ["error", "as-needed"]
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ]
  }